        <script><![CDATA[var FetchUserDetails = Class.create();
FetchUserDetails.prototype = {
    initialize: function () {},

    getUserDetails: function (email) {
        var userGR = new GlideRecord('x_snc_use_case_sco_user_info');
        userGR.addQuery('email', email);
        userGR.query();
        if (userGR.next()) {
            return {
                name: userGR.getValue('name'),
                contact: userGR.getValue('contact_number'),
                email: userGR.getValue('email')
            };
        }
        return null;
    },

    type: 'FetchUserDetails'
};
]]></script>
